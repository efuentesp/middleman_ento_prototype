---
<%= partial 'partials/page_heading' %>

<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row m-b-lg m-t-lg">
    <div class="col-md-6">
      <div class="profile-image">
        <%= image_tag current_page.data.profile.picture, resize_to: "96x96", :class => 'img-circle circle-border m-b-md', :alt => current_page.data.profile.name %>
      </div>
      <div class="profile-info">
        <div class="">
          <div>
            <h3><%= current_page.data.profile.school %> (<%= current_page.data.profile.school_level %>)</h3>
            <h2 class="no-margins"><%= current_page.data.profile.name %></h2>
            <!-- <h4><%= current_page.data.profile.role %></h4> -->
            <h5><%= current_page.data.profile.email %></h5>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5><%= current_page.data.title %></h5>
        </div>
        <div class="ibox-content">
          <div><canvas id="lineChart" height="140"></canvas></div>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5><%= data.i18n.results_learning_map %></h5>
          <div class="ibox-tools">
          </div>
        </div>
        <div class="ibox-content">
          <div>
            <div class="feed-activity-list">
              <% current_page.data.learning_map.each do |dimension| %>
                <div class="feed-element">
                  <a href="#" class="pull-left">
                    <i class="fa fa-circle <%= dimension.text_color %>"></i>
                  </a>
                  <div class="media-body ">
                    <span class="pull-right">
                      <% link_to dimension.course_url, :class => 'btn btn-primary btn-sm' do %>
                        <i class="fa fa-check"></i> <%= data.i18n.results_learning_take_course %>
                      <% end %>
                    </span>
                    <strong>Dimensión <%= dimension.dimension_number %>. <%= dimension.dimension_name %></strong><br/><br/>
                    <% link_to dimension.course_info_url do %>
                      <i class="fa fa-info-circle"></i> Curso: <%= dimension.course %>
                    <% end %>
                    <br/><br/>
                    <table class="table">
                      <thead>
                        <tr>
                          <th>Elemento de Evaluación</th>
                          <th>Puntos</th>
                          <th>Nivel</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% dimension.results.each do |result| %>
                          <tr>
                            <td><%= result.name %></td>
                            <td><%= result.points %></td>
                            <td><%= result.level %></td>
                          </tr>
                        <% end %>
                      </tbody>
                    </table>
                  </div>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
