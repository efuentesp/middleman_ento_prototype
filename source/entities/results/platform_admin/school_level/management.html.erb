---
title: Resultados por Nivel Escolar
role_index: 0

domain_singular: Nivel Escolar
domain_plural: Niveles Escolares
domain_url: school_level
domain_children:
  - name: Docentes
    id: teachers_mgmt
    link: "/entities/results/platform_admin/teacher/management"
    icon: "fa fa-caret-square-o-right"
  - name: Resultados
    id: school_results
    link: "/entities/results/platform_admin/school_level/results"
    icon: "fa fa-bar-chart"
breadcrumb:
  - name: Resultados por Escuela
    link: "/entities/results/platform_admin/school/management"
---
<%= partial 'partials/page_heading' %>
<%= partial 'partials/page_search_head', :locals => { :create_button => false } %>

<table class="table table-hover">
  <tbody>
    <% data.schools.schools[0].school_levels.each_with_index do |school_level, index| %>
      <tr>
        <td class="project-title">
          <%= data.schools.schools[0].name %>
        </td>
        <td class="project-title">
          <%= school_level.name %>
        </td>
        <%= partial 'partials/page_search_actions', :locals => { :index => index, :edit_button => false, :delete_button => false } %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= partial 'partials/pager' %>
<%= partial 'partials/page_search_foot' %>
