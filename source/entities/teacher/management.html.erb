---
title: Administrar Docentes
role_index: 0

domain_singular: Docente
domain_plural: Docentes
domain_url: teacher
breadcrumb:
  - name: Administrar Escuelas
    link: "/entities/school/management"
  - name: Administrar Niveles Escolares
    link: "/entities/school_level/management"
---
<%= partial 'partials/page_heading' %>
<%= partial 'partials/page_search_head', :locals => { :create_button => true } %>

<table class="table table-hover">
  <tbody>
    <% data.schools.schools[0].school_levels[0].teachers.each_with_index do |teacher, index| %>
      <tr>
        <td class="project-title">
          <%= data.schools.schools[0].name %>
        </td>
        <td class="project-title">
          <%= data.schools.schools[0].school_levels[0].name %>
        </td>
        <td class="project-title">
          <%= teacher.name %>
        </td>
        <%= partial 'partials/page_search_actions', :locals => { :index => index, :edit_button => true, :delete_button => true } %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= partial 'partials/pager' %>
<%= partial 'partials/page_search_foot' %>
