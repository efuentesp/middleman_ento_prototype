---
title: Administrar Niveles Escolares
role_index: 0

domain_singular: Nivel Escolar
domain_plural: Niveles Escolares
domain_url: school_level
domain_children:
  - name: Docentes
    id: teachers_mgmt
    link: "/entities/teacher/management"
    icon: "fa fa-user-circle"
breadcrumb:
  - name: Administrar Escuelas
    link: "/entities/school/management"
---
<%= partial 'partials/page_heading' %>
<%= partial 'partials/page_search_head', :locals => { :create_button => true } %>

<table class="table table-hover">
  <tbody>
    <% data.schools.schools[0].school_levels.each_with_index do |school_level, index| %>
      <tr>
        <td class="project-title">
          <%= data.schools.schools[0].name %>
        </td>
        <td class="project-title">
          <%= school_level.name %>
        </td>
        <%= partial 'partials/page_search_actions', :locals => { :index => index, :edit_button => true, :delete_button => true } %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= partial 'partials/pager' %>
<%= partial 'partials/page_search_foot' %>
